{
  "name": "Task",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "priority": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "author": {
      "type": "embedsOne",
      "model": "User",
      "property": "author",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
